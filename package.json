{
  "name": "cloud-upload-api",
  "version": "1.0.0",
  "description": "cloud storage api with folder structure",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "rm -rf dist && tsc",
    "dev": "ts-node-dev --respawn --transpile-only server.ts",
    "dev:tsc": "tsc --watch -p .",
    "dev:serve": "nodemon -e js -w dist dist/server.js",
    "test": "jest"
  },
  "keywords": [
    "node",
    "express",
    "typescript",
    "prisma",
    "aws s3",
    "multer",
    "redis",
    "postgres"
  ],
  "author": "Ifeoluwa Olubo",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "aws-sdk": "^2.1043.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^12.0.4",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.1.0",
    "helmet": "^5.0.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.3",
    "nanoid": "^3.1.30",
    "winston": "^3.4.0",
    "xss-clean": "^0.1.1",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.7",
    "@types/lodash": "^4.14.176",
    "@types/multer": "^1.4.7",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^17.0.8",
    "@types/supertest": "^2.0.11",
    "jest": "^27.4.7",
    "prisma": "^3.9.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
